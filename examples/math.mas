function main() {
  Integer a = 10;
  Integer b = 5;
 
  # Addition, answer is 15, since 15 = 10+5
  c = a + b;
  print c;

  # Subtraction, answer is 5, since 5 = 10-5
  c = a - b;
  print c;

  # Multiplication, answer is 50, since 50 = 10 * 5
  c = a * b;
  print c;

  # Division, answer is 2, since 2 = 10/5
  c = a / b;
  print c;

  # Modulus, answer is 0, since 0 = 10 % 5
  c = a % b;
  print c;

  # "Complex" Expression.
  a = 4;
  b = 2;
  c = (a + b) * 7;
  print c;
}
