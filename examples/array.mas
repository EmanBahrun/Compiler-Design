function main() {
    Array aa = [...20];
    Integer bb;
    Integer cc;


    bb = 4;
    cc = 10;
    aa[0] = bb + cc;
    print aa[0];    # should print out 14

    aa[1] = 100;
    print aa[1];    # should print out 100

    aa[2] = 200;
    print aa[2];    # should print out 200

    aa[3] = aa[0] * (aa[1] + cc);
    print aa[3];    # should print out 840; since 1540 = 14 * (100 +10)
}
