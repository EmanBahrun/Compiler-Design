function add(Integer aa, Integer bb) {
    return aa + bb;
}

function mult(Integer aa, Integer bb) {
    return aa * bb;
}

function main() {
    Integer aa = 100;
    Integer bb = 50;
    Integer cc = add(aa, bb);
    Integer dd = 0;
    print cc; # should print 150

    dd = mult(cc, bb);
    print dd; # should print "7500", since 7500 = 150 * 150;
}