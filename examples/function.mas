function add(Integer aa, Integer bb) {
    return aa + bb;
}

function mult(Integer aa, Integer bb) {
    return aa * bb;
}

function main() {
    Integer aa = 100;
    Integer bb = 50;
    Integer c = add(aa, bb);
    print c; # should print 150

    d = mult(c, aa + bb);
    print d; # should print "22500", since 22500 = 150 * 150;
}